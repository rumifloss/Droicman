    }

    //Left free check
    if ( direction == right &amp;&amp; !place_meeting(x, y - 16, obj_wall) ) || ( direction == up &amp;&amp; !place_meeting(x - 16, y, obj_wall) ) || ( direction == left &amp;&amp; !place_meeting(x, y + 16, obj_wall) ) || ( direction == down &amp;&amp; !place_meeting(x + 16, y, obj_wall) )
    {
        free_left = 1;
    }
    else
    {
        free_left = 0;
    }

    //Right free check
    if ( direction == right &amp;&amp; !place_meeting(x, y + 16, obj_wall) ) || ( direction == up &amp;&amp; !place_meeting(x + 16, y, obj_wall) ) || ( direction == left &amp;&amp; !place_meeting(x, y - 16, obj_wall) ) || ( direction == down &amp;&amp; !place_meeting(x - 16, y, obj_wall) )
    {
        free_right = 1;
    }
    else
